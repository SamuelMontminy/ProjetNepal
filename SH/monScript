#!/bin/bash
# MyApp
#
# description: bla bla

case $1 in
    start)
        /bin/bash /home/pi/ProjetNepal/RPM-start.sh
    ;;
    stop)
        /bin/bash /home/pi/ProjetNepal/RPM-stop.sh
    ;;
    restart)
        /bin/bash /home/pi/ProjetNepal/RPM-stop.sh
        /bin/bash /home/pi/ProjetNepal/RPM-start.sh
    ;;
esac
exit 0
